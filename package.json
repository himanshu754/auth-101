{
  "name": "auth-101",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dist": "sh ./scripts/dist.sh",
    "start": "node dist/main.js",
    "dev": "nodemon",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d data-source-cli.ts",
    "migration:create": "typeorm migration:create ./src/migrations/$npm_config_name",
    "migration:generate": "npm run typeorm -- migration:generate ./src/migrations/$npm_config_name",
    "migration:show": "npm run build && npm run typeorm migration:show",
    "migration:up": "npm run build && npm run typeorm -- migration:run",
    "migration:down": "npm run typeorm migration:revert",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "cron2": "tsc && node src/utils/cron2_func.js",
    "test": "jest"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ]
  },
  "dependencies": {
    "@hubspot/api-client": "^9.1.1",
    "@types/uuid": "^9.0.8",
    "aws-sdk": "^2.1348.0",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.19.2",
    "express-validator": "^6.14.0",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.1.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^3.1.2",
    "luxon": "^2.4.0",
    "node-cron": "^3.0.2",
    "npm": "^10.5.2",
    "owasp-password-strength-test": "^1.3.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "redis": "^4.6.12",
    "strip-tags": "^0.1.1",
    "tsconfig-paths": "^4.1.1",
    "typeorm": "^0.3.12",
    "uuid": "^9.0.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/luxon": "^2.3.2",
    "@types/node": "^17.0.41",
    "@types/node-cache": "^4.2.5",
    "@types/node-cron": "^3.0.7",
    "@types/owasp-password-strength-test": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-security": "^2.1.1",
    "eslint-plugin-unused-imports": "^3.1.0",
    "husky": "^8.0.1",
    "jest": "^29.5.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.8.1",
    "tsc-alias": "^1.6.9",
    "typescript": "^4.7.3"
  },
  "author": "",
  "license": "ISC"
}
